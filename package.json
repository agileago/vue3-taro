{
  "name": "taro3-vue3-template",
  "version": "1.0.0",
  "private": true,
  "description": "taro3-vue3-template",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "dev:h5": "pnpm run build:h5 -- --watch",
    "dev:weapp": "pnpm run build:weapp -- --watch",
    "build:h5": "taro build --type h5",
    "build:weapp": "taro build --type weapp",
    "tsc": "tsc  --noEmit",
    "lint": "eslint --fix .",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@nutui/nutui-taro": "3.1.17",
    "@tarojs/components": "3.4.3",
    "@tarojs/taro": "3.4.3",
    "injection-js": "^2.4.0",
    "taro-axios": "^1.1.1",
    "tslib": "^2.3.1",
    "vue": "^3.0.0",
    "vue3-oop": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/runtime": "^7.7.7",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@tarojs/cli": "3.4.3",
    "@tarojs/mini-runner": "3.4.3",
    "@tarojs/plugin-framework-vue3": "3.4.3",
    "@tarojs/plugin-html": "3.4.3",
    "@tarojs/runtime": "3.4.3",
    "@tarojs/webpack-runner": "3.4.3",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "babel-plugin-import": "^1.13.3",
    "babel-preset-taro": "3.4.3",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.3",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "lint-staged": "^12.3.5",
    "@milahu/patch-package-with-pnpm-support": "^6.4.8",
    "prettier": "^2.5.1",
    "ts-loader": "~8.2.0",
    "typescript": "^4.1.0",
    "vue-loader": "^17.0.0",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,html,json}": [
      "prettier --write"
    ]
  }
}
