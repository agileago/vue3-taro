{
  "name": "taro3-vue3-template",
  "version": "1.0.0",
  "private": true,
  "description": "taro3-vue3-template",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "dev": "pnpm run build --watch",
    "dev:prod": "NODE_ENV=production pnpm run build --watch",
    "build": "taro build --type weapp",
    "build:master": "pnpm run build --mode master",
    "dev:h5": "pnpm run build:h5 --watch",
    "build:h5": "taro build --type h5",
    "preview:h5": "serve -s dist",
    "type:check": "tsc --noEmit",
    "lint": "eslint --fix .",
    "postinstall": "simple-git-hooks"
  },
  "dependencies": {
    "@abraham/reflection": "^0.11.0",
    "@babel/runtime": "^7.20.13",
    "@nutui/nutui-taro": "^4.0.0",
    "@tarojs/components": "3.6.0",
    "@tarojs/helper": "3.6.0",
    "@tarojs/plugin-framework-vue3": "3.6.0",
    "@tarojs/plugin-html": "3.6.0",
    "@tarojs/plugin-platform-h5": "^3.6.0",
    "@tarojs/plugin-platform-weapp": "3.6.0",
    "@tarojs/runtime": "3.6.0",
    "@tarojs/shared": "3.6.0",
    "@tarojs/taro": "3.6.0",
    "@vue3-oop/taro-hooks": "^1.0.1",
    "axios": "^1.3.1",
    "injection-js": "^2.4.0",
    "path-to-regexp": "^6.2.1",
    "ts-essentials": "^9.3.0",
    "tslib": "^2.5.0",
    "vue": "^3.2.47",
    "vue3-oop": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@tarojs/cli": "^3.6.0",
    "@tarojs/plugin-http": "^3.6.0",
    "@tarojs/plugin-mock": "^0.0.9",
    "@tarojs/webpack5-runner": "3.6.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue3-oop/taro-plugin": "^1.0.4",
    "babel-plugin-import": "^1.13.6",
    "babel-preset-taro": "3.6.0",
    "cross-env": "^7.0.3",
    "css-loader": "6.7.3",
    "eslint": "~8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-taro": "3.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.9.0",
    "lint-staged": "^13.1.0",
    "lodash": "^4.17.21",
    "postcss": "^8.4.21",
    "postcss-preset-env": "^8.0.1",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "serve": "^14.2.0",
    "simple-git-hooks": "^2.8.1",
    "style-loader": "3.3.1",
    "swagger-schema-official": "2.0.0-bab6bed",
    "tailwindcss": "~3.2.4",
    "tailwindcss-rem2px-preset": "^1.0.3",
    "ts-gear": "^4.11.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.0.4",
    "vue-loader": "^17.0.1",
    "weapp-tailwindcss-webpack-plugin": "^1.12.8",
    "webpack": "5.75.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged",
    "commit-msg": "pnpm exec commitlint -e \"$@\""
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "*.{css,scss,html,json}": [
      "prettier --write"
    ]
  }
}
