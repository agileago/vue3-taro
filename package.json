{
  "name": "taro3-vue3-template",
  "version": "1.0.0",
  "private": true,
  "description": "taro3-vue3-template",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "dev:weapp": "pnpm run build:weapp -- --watch",
    "uat:weapp": "cross-env MODE=uat pnpm run build:weapp",
    "build:weapp": "taro build --type weapp",
    "dev:h5": "pnpm run build:h5 -- --watch",
    "uat:h5": "cross-env MODE=uat pnpm run build:h5",
    "build:h5": "taro build --type h5",
    "preview:h5": "serve -s dist",
    "type:check": "tsc --noEmit",
    "lint": "eslint --fix .",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@nutui/nutui-taro": "3.1.18",
    "@tarojs/components": "3.4.4",
    "@tarojs/taro": "3.4.4",
    "injection-js": "^2.4.0",
    "taro-axios": "^1.1.1",
    "ts-essentials": "^9.1.2",
    "tslib": "^2.3.1",
    "vue": "^3.2.31",
    "vue3-oop": "^0.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/runtime": "^7.17.9",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@milahu/patch-package-with-pnpm-support": "^6.4.8",
    "@tarojs/cli": "3.4.4",
    "@tarojs/mini-runner": "3.4.4",
    "@tarojs/plugin-framework-vue3": "^3.4.4",
    "@tarojs/plugin-html": "3.4.4",
    "@tarojs/runner-utils": "^3.4.4",
    "@tarojs/runtime": "3.4.4",
    "@tarojs/webpack-runner": "3.4.4",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@vue3-oop/babel-plugin-jsx": "^1.1.1",
    "@vue3-oop/jsx-hot-loader": "^0.1.6",
    "autoprefixer": "^10.4.4",
    "babel-plugin-import": "^1.13.3",
    "babel-preset-taro": "3.4.4",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.3",
    "cross-env": "^7.0.3",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "6",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.2",
    "serve": "^13.0.2",
    "ts-deepmerge": "^2.0.1",
    "ts-gear": "^4.6.1",
    "ts-loader": "~8.2.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vue-loader": "^17.0.0",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,html,json}": [
      "prettier --write"
    ]
  }
}
