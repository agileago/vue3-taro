{
  "name": "taro3-vue3-template",
  "version": "1.0.0",
  "private": true,
  "description": "taro3-vue3-template",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "dev": "pnpm run build --watch",
    "dev:prod": "NODE_ENV=production pnpm run build --watch",
    "build": "taro build --type weapp",
    "build:master": "pnpm run build --mode master",
    "dev:h5": "pnpm run build:h5 --watch",
    "build:h5": "taro build --type h5",
    "preview:h5": "serve -s dist",
    "type:check": "tsc --noEmit",
    "lint": "eslint --fix .",
    "api": "tsg -c ./src/api/tsg.config.ts",
    "postinstall": "simple-git-hooks && weapp-tw patch"
  },
  "dependencies": {
    "@abraham/reflection": "^0.12.0",
    "@babel/runtime": "^7.22.10",
    "@nutui/nutui-taro": "^4.1.3",
    "@tarojs/components": "3.6.13",
    "@tarojs/helper": "3.6.13",
    "@tarojs/plugin-framework-vue3": "3.6.13",
    "@tarojs/plugin-html": "3.6.13",
    "@tarojs/plugin-platform-h5": "^3.6.13",
    "@tarojs/plugin-platform-weapp": "3.6.13",
    "@tarojs/runtime": "3.6.13",
    "@tarojs/shared": "3.6.13",
    "@tarojs/taro": "3.6.13",
    "@vue3-oop/taro-hooks": "^1.0.1",
    "axios": "^1.4.0",
    "injection-js": "^2.4.0",
    "path-to-regexp": "^6.2.1",
    "ts-essentials": "^9.3.2",
    "tslib": "^2.6.1",
    "vue": "^3.3.4",
    "vue3-oop": "^1.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@tarojs/cli": "^3.6.13",
    "@tarojs/plugin-http": "^3.6.13",
    "@tarojs/plugin-mock": "^0.0.9",
    "@tarojs/webpack5-runner": "3.6.13",
    "@types/lodash": "^4.14.197",
    "@types/node": "^20.5.0",
    "@types/webpack-env": "^1.18.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vue/babel-plugin-jsx": "^1.1.5",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue3-oop/taro-plugin": "^1.0.5",
    "babel-plugin-import": "^1.13.8",
    "babel-preset-taro": "3.6.13",
    "cross-env": "^7.0.3",
    "css-loader": "6.8.1",
    "eslint": "~8.47.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-config-taro": "3.6.13",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "lint-staged": "^14.0.0",
    "lodash": "^4.17.21",
    "postcss": "^8.4.27",
    "postcss-preset-env": "^9.1.1",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "serve": "^14.2.0",
    "simple-git-hooks": "^2.9.0",
    "style-loader": "3.3.3",
    "swagger-schema-official": "2.0.0-bab6bed",
    "tailwindcss": "~3.3.3",
    "tailwindcss-rem2px-preset": "^1.0.3",
    "ts-gear": "^4.11.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vue-loader": "^17.2.2",
    "weapp-tailwindcss": "^2.8.0",
    "webpack": "^5.88.2"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged",
    "commit-msg": "pnpm exec commitlint -e \"$@\""
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "*.{css,scss,html,json}": [
      "prettier --write"
    ]
  }
}
